{
  "name": "short-url",
  "version": "1.0.0",
  "description": "short url",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/index.js --exec babel-node",
    "build": "cross-env NODE_ENV=production babel src --out-dir dist && npm run copy",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "lint": "eslint . --fix",
    "copy": "copyfiles --up 1 src/public/* src/public/css/* src/public/js/* src/public/.well-known/* src/public/.well-known/pki-validation/* src/views/** dist",
    "docs": "jsdoc -c jsdoc.json"
  },
  "repository": "git+https://github.com/LuisFOsG/short-url.git",
  "keywords": [],
  "author": "LuisFOsG",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuisFOsG/short-url/issues"
  },
  "homepage": "https://github.com/LuisFOsG/short-url#readme",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "core-js": "3.9.1",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "ejs": "3.1.6",
    "express": "4.17.1",
    "helmet": "4.4.1",
    "method-override": "3.0.0",
    "mongoose": "5.12.2",
    "morgan": "1.10.0",
    "qrcode": "1.4.4",
    "regenerator-runtime": "0.13.7",
    "sass": "1.32.8",
    "url-exists": "1.0.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.8",
    "@babel/node": "7.13.0",
    "@babel/preset-env": "7.13.9",
    "copyfiles": "2.4.1",
    "eslint": "7.22.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "jsdoc": "3.6.6",
    "nodemon": "2.0.7"
  }
}
